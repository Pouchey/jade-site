{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "jsx": "react-jsx",
    "noEmit": true,
    "paths": {
      "_components/*": [
        "./src/components/*"
      ],
      "_hooks/*": [
        "./src/hooks/*"
      ],
      "_modules/*": [
        "./src/modules/*"
      ],
      "_services/*": [
        "./src/services/*"
      ],
      "_style/*": [
        "./src/style/*"
      ],
    }
  },
  "include": [
    "src",
    "vite.config.ts",
  ],
}
